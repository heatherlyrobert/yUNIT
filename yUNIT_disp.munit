   yUNIT_minstr   ("... check header"                   , yENV_upeek  (myUNIT.name, '>'), "      test   : s_equal    (rc =    0, test abbr = -)");
   yUNIT_minstr   ("... check end"                      , yENV_upeek  (myUNIT.name, '>'), "(n/a)");
