   yUNIT_minval   ("call mid lines"                     , yunit_disp__mid  ("critical_function"      , "15, 27, \"text\""                  , "s_equal"  ),   0);
   yUNIT_minval   ("close out a unit test"              , yUNIT_tinu  (1), 0);
   yUNIT_mindnoc  ();

   yUNIT_mincond  ("verify will not act on SCRP");
