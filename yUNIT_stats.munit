   yUNIT_minstr  ("... check result"                   , yENV_upeek      (s_nconv  , '>')  , "   #========  =================================================================  ID   TOP===all====glob====shar====scrp   MID===all====cond====ditto=and=usage==========local=reuse=====================global=reuse=================skip   BOT===all====real====vars====void====skip    =================  ");
   yUNIT_minstr  ("... check result"                   , yENV_upeek      (s_nconv  , '>')  , "   PRCS       script statistics -----------------------------------------------   2         1     и     и     1        10     1     2  иии2ииииии     4  fza                      3  DЫ                       0         0     0     0     0     0  -----------------  ");
   yUNIT_minstr  ("... check result"                   , yENV_upeek      (s_nconv  , '>')  , ""                                         );
   yUNIT_minstr  ("... check result"                   , yENV_upeek      (s_nconv  , '>')  , "#===========  =================================================================  ID   TOP===all====glob====shar====scrp   MID===all====cond====ditto=and=usage==========local=reuse=====================global=reuse=================skip   BOT===all====real====vars====void====skip    =================  ");
