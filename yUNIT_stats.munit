   yUNIT_minval  ("... verify script number"           , x_cscrp    ,    0);
   yUNIT_minval  ("... verify condition number"        , x_ccond    ,    0);
   yUNIT_minval  ("... verify step number"             , x_cstep    ,    0);
   yUNIT_mindnoc ();

   yUNIT_mincond ("close file");
   yUNIT_minptr  ("open file"                          , fclose (f)               ,     0);
