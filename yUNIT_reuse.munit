#include    "master.munit"



char
UNIT___reuse_ftype      (char a_scrp, char a_list, char a_unit)
{
   char        x_header    [LEN_TITLE] = "";
   /*---(header)-------------------------*/
   yUNIT_minscrpy ("koios", "koios_reuse", "(REUSE) identifying resue file type");
   if (a_list == '!')    { yUNIT_minprcs (); return 0; }
   if (a_unit != 0 && a_scrp != a_unit)  return 0;
   if (a_list == 'y')                    return 0;

   yUNIT_mincond ("attempt problems");
   yUNIT_minchr  ("run ftype"                          , yUNIT_reuse_ftype  (NULL            , x_header),  '░');
   yUNIT_minstr  ("... review header"                  , x_header                                       ,  "");
   yUNIT_minchr  ("run ftype"                          , yUNIT_reuse_ftype  (""              , x_header),  '-');
   yUNIT_minstr  ("... review header"                  , x_header                                       ,  "");
   yUNIT_minchr  ("run ftype"                          , yUNIT_reuse_ftype  ("unit_head.unit", NULL    ),  'h');
   yUNIT_mindnoc ();

   yUNIT_mincond ("verify on shared types");
   yUNIT_minchr  ("run ftype"                          , yUNIT_reuse_ftype  ("unit_head.unit", x_header),  'h');
   yUNIT_minstr  ("... review header"                  , x_header                                       ,  "");
   yUNIT_minchr  ("run ftype"                          , yUNIT_reuse_ftype  ("unit_wide.unit", x_header),  'w');
   yUNIT_minstr  ("... review header"                  , x_header                                       ,  "unit_wide.h");
   yUNIT_minchr  ("run ftype"                          , yUNIT_reuse_ftype  ("unit_data.unit", x_header),  'd');
   yUNIT_minstr  ("... review header"                  , x_header                                       ,  "unit_data.h");
   yUNIT_mindnoc ();

   yUNIT_mincond ("attempt non-shared files");
   yUNIT_minchr  ("run ftype"                          , yUNIT_reuse_ftype  ("poly_base.unit", x_header),  '-');
   yUNIT_minstr  ("... review header"                  , x_header                                       ,  "");
   yUNIT_minchr  ("run ftype"                          , yUNIT_reuse_ftype  ("poly_base.c"   , x_header),  '-');
   yUNIT_minstr  ("... review header"                  , x_header                                       ,  "");
   yUNIT_minchr  ("run ftype"                          , yUNIT_reuse_ftype  ("Makefile"      , x_header),  '-');
   yUNIT_minstr  ("... review header"                  , x_header                                       ,  "");
   yUNIT_mindnoc ();

   yUNIT_minprcs ();
   return 0;
}

char
UNIT___reuse_type       (char a_scrp, char a_list, char a_unit)
{
   /*---(header)-------------------------*/
   yUNIT_minscrpy ("koios", "koios_reuse", "(REUSE) identifying reuse category");
   if (a_list == '!')    { yUNIT_minprcs (); return 0; }
   if (a_unit != 0 && a_scrp != a_unit)  return 0;
   if (a_list == 'y')                    return 0;

   yUNIT_mincond ("verify on base run-time");
   yUNIT_minchr  ("... run full"                       , yUNIT_reuse_type  ('0'),  '-');
   yUNIT_minchr  ("... run unit"                       , yUNIT_reuse_type  ('1'),  '-');
   yUNIT_minchr  ("... run scrp"                       , yUNIT_reuse_type  ('2'),  '-');
   yUNIT_minchr  ("... run cond"                       , yUNIT_reuse_type  ('3'),  '-');
   yUNIT_minchr  ("... run audit"                      , yUNIT_reuse_type  ('!'),  '-');
   yUNIT_mindnoc ();

   yUNIT_mincond ("verify on local shared");
   yUNIT_minchr  ("... run a"                          , yUNIT_reuse_type  ('a'),  's');
   yUNIT_minchr  ("... run e"                          , yUNIT_reuse_type  ('e'),  's');
   yUNIT_minchr  ("... run q"                          , yUNIT_reuse_type  ('q'),  's');
   yUNIT_minchr  ("... run z"                          , yUNIT_reuse_type  ('z'),  's');
   yUNIT_mindnoc ();

   yUNIT_mincond ("verify null handling");
   yUNIT_minchr  ("... run NULL"                       , yUNIT_reuse_type  ( 0 ),  '-');
   yUNIT_mindnoc ();

   yUNIT_mincond ("verify on global shared");
   yUNIT_minchr  ("... run A"                          , yUNIT_reuse_type  ('A'),  'g');
   yUNIT_minchr  ("... run L"                          , yUNIT_reuse_type  ('L'),  'g');
   yUNIT_minchr  ("... run Y"                          , yUNIT_reuse_type  ('Y'),  'g');
   yUNIT_mindnoc ();

   yUNIT_mincond ("verify defaultish");
   yUNIT_minchr  ("... run space"                      , yUNIT_reuse_type  (' '),  '-');
   yUNIT_minchr  ("... run -"                          , yUNIT_reuse_type  ('-'),  '-');
   yUNIT_minchr  ("... run и"                          , yUNIT_reuse_type  ('и'),  '-');
   yUNIT_mindnoc ();

   yUNIT_mincond ("verify on global config");
   yUNIT_minchr  ("... run Ж"                          , yUNIT_reuse_type  ('Ж'),  'c');
   yUNIT_minchr  ("... run  "                          , yUNIT_reuse_type  (' '),  'c');
   yUNIT_mindnoc ();

   yUNIT_mincond ("attempt invalid indexes");
   yUNIT_minchr  ("... run ?"                          , yUNIT_reuse_type  ('5'),  '-');
   yUNIT_minchr  ("... run +"                          , yUNIT_reuse_type  ('+'),  '-');
   yUNIT_minchr  ("... run ┤"                          , yUNIT_reuse_type  ('┤'),  '-');
   yUNIT_minchr  ("... run ┤"                          , yUNIT_reuse_type  ('='),  '-');
   yUNIT_mindnoc ();

   yUNIT_minprcs ();
   return 0;
}

