char
UNIT___reuse_title      (char a_scrp, char a_list, char a_unit)
{
   char        rc          =    0;

   /*---(header)-------------------------*/
   yUNIT_minscrpy ("koios", "koios_reuse", "(REUSE) verify creating shared titles");
   if (a_list == '!')    { yUNIT_minprcs (); return 0; }
   if (a_unit != 0 && a_scrp != a_unit)  return 0;
   if (a_list == 'y')                    return 0;

   yUNIT_mincond ("verify adding data");
   yUNIT_minval  ("... parse"                          , yUNIT_reuse_parse  ('g',    "b  s  -    122  check detailed data                                                   1     0     0     1     6     4     2  и2ииииииии     0  и                        0  и                        0     14     10      1      3      0      8      6      2      0    тabcииfииииииТ  a=testии, b=extraи, c=fullии "),    1);
   yUNIT_mindnoc ();

   yUNIT_mincond ("verify legal settings");
   yUNIT_minstr  ("... call title"                     , yUNIT_reuse_title  ('b', 'a'), "===[[ check detailed data (a=test) ]]============================");
   yUNIT_mindnoc ();

   yUNIT_mincond ("attempt with illegal share");
   yUNIT_minstr  ("... call title"                     , yUNIT_reuse_title  ('?', 'a'), "===[[ (n/a) ]]===================================================");
   yUNIT_mindnoc ();

   yUNIT_mincond ("attempt with undefined share");
   yUNIT_minstr  ("... call title"                     , yUNIT_reuse_title  ('a', 'a'), "===[[ (unset) ]]=================================================");
   yUNIT_mindnoc ();

   yUNIT_mincond ("attempt with null select");
   yUNIT_minstr  ("... call title"                     , yUNIT_reuse_title  ('b',  0 ), "===[[ check detailed data (NULL) ]]==============================");
   yUNIT_mindnoc ();

   yUNIT_mincond ("attempt with undefined select");
   yUNIT_minstr  ("... call title"                     , yUNIT_reuse_title  ('b', 'k'), "===[[ check detailed data (k=N/A) ]]=============================");
   yUNIT_mindnoc ();

   yUNIT_mincond ("attempt with untitled select");
   yUNIT_minstr  ("... call title"                     , yUNIT_reuse_title  ('b', 'f'), "===[[ check detailed data (f=TBD) ]]=============================");
   yUNIT_mindnoc ();

   yUNIT_mincond ("verify another legal settings");
   yUNIT_minstr  ("... call title"                     , yUNIT_reuse_title  ('b', 'c'), "===[[ check detailed data (c=full) ]]============================");
   yUNIT_mindnoc ();

   yUNIT_mincond ("verify another wildcard select");
   yUNIT_minstr  ("... call title"                     , yUNIT_reuse_title  ('b', '*'), "===[[ check detailed data (EVERY) ]]=============================");
   yUNIT_mindnoc ();

   yUNIT_minprcs ();
   return 0;
}

